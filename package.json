{
  "name": "gmk87-hid-uploader",
  "version": "1.0.0",
  "description": "Node.js utilities for uploading images and syncing time on GMK87 keyboard displays via HID protocol",
  "type": "module",
  "main": "src/timesync.js",
  "scripts": {
    "timesync": "node src/timesync.js",
    "upload": "node src/uploadImage.js",
    "sendimage": "node src/sendImageMagick.js",
    "lights": "node src/configureLights.js",
    "preset": "node src/loadPreset.js",
    "examples": "node src/examples.js"
  },
  "bin": {
    "gmk87-timesync": "./src/timesync.js",
    "gmk87-upload": "./src/uploadImage.js",
    "gmk87-sendimage": "./src/sendImageMagick.js",
    "gmk87-lights": "./src/configureLights.js",
    "gmk87-preset": "./src/loadPreset.js",
    "gmk87-examples": "./src/examples.js"
  },
  "dependencies": {
    "jimp": "^0.22.10",
    "node-hid": "^2.2.0",
    "usb": "^2.16.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "gmk87",
    "hid",
    "keyboard",
    "display",
    "image-upload",
    "time-sync"
  ],
  "author": "Codedgar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codedgar/gmk87-node"
  }
}
